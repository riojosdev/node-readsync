{
  "name": "read-sync",
  "version": "0.0.1",
  "description": "A minimalistic & lightweight notification system, built using Service Workers and NodeJS",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon -L app.js",
    "lint": "npx eslint ./ --fix",
    "lint-dev": "npm run lint && npm run dev",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo"
  },
  "keywords": [
    "Notification",
    "Service Worker"
  ],
  "author": "<www.decodedHTML.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "googleapis": "^111.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "sequelize": "^6.28.0",
    "uuid": "^9.0.0",
    "web-push": "^3.5.0"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.6.0"
  }
}
